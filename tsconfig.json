{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "dist",
    "moduleResolution": "node",
    "lib": [
      "es2016",
      "es2017",
      "esnext.asynciterable",
      "dom"
    ],
    "types": ["node", "jest"]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "**/*.ts"
  ]
}
